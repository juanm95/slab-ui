{
  "manifest_version": 3,
  "name": "Embedded Angular App",
  "version": "1.0",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["http://192.168.120.240/"],
  "web_accessible_resources": [
    {
      "resources": ["browser/*"],
      "matches": ["http://192.168.120.240/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["http://192.168.120.240/"],
      "js": ["content.js"]
    }
  ]
}